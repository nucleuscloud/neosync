{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "apps/web/.next/**",
        "!apps/web/.next/cache/**",
        "packages/sdk/dist"
      ]
    },
    "deploy": {
      "dependsOn": ["build"]
    },
    "lint": {},
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "typecheck": {},
    "test": {
      "dependsOn": ["^build"]
    }
  }
}
