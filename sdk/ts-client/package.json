{
  "name": "@neosync/sdk",
  "version": "0.0.0",
  "description": "Neosync SDK",
  "files": [
    "./dist/**/*"
  ],
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run tsup",
    "tsup": "tsup src/index.ts --format cjs,esm --dts",
    "prepare": "npm run tsup"
  },
  "author": "nickzelei",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^1.5.1",
    "@connectrpc/connect": "^1.1.4"
  }
}
